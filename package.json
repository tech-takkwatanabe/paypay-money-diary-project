{
	"name": "paypay-money-diary-monorepo",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"fmt": "prettier --write \"**/*.{ts,tsx,md}\"",
		"clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
	},
	"devDependencies": {
		"prettier": "3.7.4",
		"turbo": "2.6.3"
	},
	"packageManager": "pnpm@10.24.0",
	"pnpm": {
		"overrides": {
			"@esbuild-kit/core-utils": "npm:tsx@^4.19.2",
			"@esbuild-kit/esm-loader": "npm:tsx@^4.19.2"
		},
		"ignoredBuiltDependencies": [
			"esbuild",
			"sharp",
			"unrs-resolver"
		]
	},
	"engines": {
		"node": ">=23"
	},
	"dependencies": {
		"zod": "4.1.13"
	}
}
