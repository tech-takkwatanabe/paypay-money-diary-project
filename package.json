{
  "name": "paypay-money-diary-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "pnpm -r test",
    "fmt": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "devDependencies": {
    "prettier": "3.8.0",
    "turbo": "2.7.5"
  },
  "packageManager": "pnpm@10.29.2",
  "pnpm": {
    "overrides": {
      "@esbuild-kit/core-utils": "npm:tsx@^4.19.2",
      "@esbuild-kit/esm-loader": "npm:tsx@^4.19.2"
    },
    "ignoredBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "engines": {
    "node": ">=24"
  },
  "dependencies": {
    "zod": "4.3.6"
  }
}
